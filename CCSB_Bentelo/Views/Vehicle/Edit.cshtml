@model CCSB_Bentelo.Models.Vehicle
<div>
    <div class="loginPage">
        <div class="loginContainer">

            <h4>Vehicle</h4>
            <hr />
            <div class="row">
                <div class="col-md-4">
                    <form asp-action="Edit">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <input type="hidden" asp-for="VehicleId" />
                        <div class="form-group">
                            <label asp-for="VehicleBrand" class="control-label">Voertuig Merk</label>
                            <input asp-for="VehicleBrand" class="form-control" />
                            <span asp-validation-for="VehicleBrand" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="VehicleType" class="control-label">Voertuig Soort</label>
                            <input asp-for="VehicleType" class="form-control" />
                            <span asp-validation-for="VehicleType" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Length" class="control-label">Lengte</label>
                            <input asp-for="Length" class="form-control" />
                            <span asp-validation-for="Length" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="VehicleKind" class="control-label">Voertuig Status</label>
                            <input asp-for="VehicleKind" class="form-control" />
                            <span asp-validation-for="VehicleKind" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="LicensePlate" class="control-label">Voertuig Kenten</label>
                            <input asp-for="LicensePlate" class="form-control" />
                            <span asp-validation-for="LicensePlate" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="PostalCode" class="control-label">Postcode</label>
                            <input asp-for="PostalCode" class="form-control" />
                            <span asp-validation-for="PostalCode" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Address" class="control-label">Adres</label>
                            <input asp-for="Address" class="form-control" />
                            <span asp-validation-for="Address" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="City" class="control-label">Stad</label>
                            <input asp-for="City" class="form-control" />
                            <span asp-validation-for="City" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="PhoneNumber" class="control-label">Telefoon Nummer</label>
                            <input asp-for="PhoneNumber" class="form-control" />
                            <span asp-validation-for="PhoneNumber" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="DatumGemaakt" class="control-label">Datum</label>
                            <input asp-for="DatumGemaakt" class="form-control" />
                            <span asp-validation-for="DatumGemaakt" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Color" class="control-label">Kleur</label>
                            <input asp-for="Color" class="form-control" />
                            <span asp-validation-for="Color" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Year" class="control-label">Bouwjaar</label>
                            <input asp-for="Year" class="form-control" />
                            <span asp-validation-for="Year" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Beds" class="control-label">Aantal slaappatsen</label>
                            <input asp-for="Beds" class="form-control" />
                            <span asp-validation-for="Beds" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Bicycle" class="control-label">Fietsdrager</label>
                            <input asp-for="Bicycle" class="form-control" />
                            <span asp-validation-for="Bicycle" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Airco" class="control-label"></label>
                            <input asp-for="Airco" class="form-control" />
                            <span asp-validation-for="Airco" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Km" class="control-label">KliometerStand</label>
                            <input asp-for="Km" class="form-control" />
                            <span asp-validation-for="Km" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Power" class="control-label">Aantal PK's</label>
                            <input asp-for="Power" class="form-control" />
                            <span asp-validation-for="Power" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Type" class="control-label">Soort (Intergaal, Alkoof of Halfintergaal)</label>
                            <input asp-for="Type" class="form-control" />
                            <span asp-validation-for="Type" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Tow" class="control-label">Trekhaak</label>
                            <input asp-for="Tow" class="form-control" />
                            <span asp-validation-for="Tow" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Weight" class="control-label">Ledig gewicht</label>
                            <input asp-for="Weight" class="form-control" />
                            <span asp-validation-for="Weight" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Water" class="control-label">VulwaterTank</label>
                            <input asp-for="Water" class="form-control" />
                            <span asp-validation-for="Water" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="ApplicationUserId" class="control-label">Klant</label>
                            <select asp-for="ApplicationUserId" class="form-control" asp-items="ViewBag.ApplicationUserId"></select>
                        </div>
                        <div class="form-group">
                            <input type="submit" value="Save" class="btn btn-primary" />
                        </div>
                    </form>
                </div>
            </div>

            <div>
                <a asp-action="Index">Back to List</a>
            </div>

        </div>
    </div>
</div>